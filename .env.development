# Development Environment Configuration
NODE_ENV=development
PORT=5001
HOST=localhost

# Database Configuration
DATABASE_URL=postgresql://postgres:dev_password@localhost:5432/aiagent_dev
POSTGRES_DB=aiagent_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=dev_password
DB_POOL_MAX=10
DB_POOL_MIN=2

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_DB=0

# Qdrant Configuration
QDRANT_URL=http://localhost:6333

# JWT Configuration
JWT_SECRET=development_jwt_secret_key_minimum_32_characters_long
JWT_EXPIRES_IN=24h

# CORS Configuration
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Rate Limiting (Lenient for development)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Logging
LOG_LEVEL=debug
LOG_FORMAT=dev
LOG_FILE=./logs/development.log

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads/dev

# AI Services (Use test keys or mock services)
OPENAI_API_KEY=sk-test-key-for-development
ANTHROPIC_API_KEY=test-key-for-development
OPENAI_MODEL=gpt-3.5-turbo

# Email (Use test service like Mailtrap)
SMTP_HOST=smtp.mailtrap.io
SMTP_PORT=2525
SMTP_USER=your_mailtrap_user
SMTP_PASS=your_mailtrap_password
EMAIL_FROM=dev@aiagent.local

# Monitoring (Disabled in development)
SENTRY_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0